<!-- This file contains information telling the splitter how to -->
<!-- turn the large manual.xml file into manageble pieces. -->
<!-- $Id: structure.xml,v 1.7 2001/07/27 15:39:37 nilsson Exp $ -->

<dir name="manual">
  <dir name="Process">
    <file name="create_process.xml">
      <!-- Only list create() and limit_value at -->
      <!-- this page and list them in that order -->
      <target module="Process" class="create_process" entity="create"/>
      <target module="Process" class="create_process" entity="limit_value"/>
    </file>
    <file name="index.xml">
      <!-- The rest of the Process module and its contents, perhaps -->
      <!-- also including Process.create_process.* things not above -->
      <target module="Process"/>
    </file>
  </dir>
  <dir name="Protocols">
    <dir name="HTTP">
      <file name="index.xml">
        <!-- Everything below Protocols.HTTP, document order: -->
        <target module="Protocols.HTTP"/>
      </file>
    </dir>
    <file name="index.xml">
      <!-- Similarly, the rest of the Protocols module goes here: -->
      <target module="Protocols"/>
    </file>
  </dir>
  <dir name="Standards">
    <file name="URI.xml">
      <!-- All from Standards.URI, document order: -->
      <target module="Standards" class="URI"/>
    </file>
  </dir>
  <file name="Array.xml">
    <target module="Array"/>
  </file>
  <file name="Calendar.xml">
    <target module="Calendar"/>
  </file>
  <file name="Crypto.xml">
    <target module="Crypto"/>
  </file>
  <file name="GDK.xml">
    <target module="GDK"/>
  </file>
  <file name="Geography.xml">
    <target module="Geography"/>
  </file>
  <file name="Gnome.xml">
    <target module="Gnome"/>
  </file>
  <!--
  <file name="GL.xml">
    <target module="GL"/>
    <target module="GLU"/>
    <target module="GLUT"/>
  </file>
  -->
  <file name="GTK.xml">
    <target module="GTK"/>
  </file>
  <file name="Gz.xml">
    <target module="Gz"/>
  </file>
  <file name="Image.xml">
    <target module="Image"/>
    <target module="_Image"/>
  </file>
  <file name="MIME.xml">
    <target module="MIME"/>
  </file>
  <file name="Database.xml">
    <target module="Sql"/>
    <target module="Mysql"/>
    <target module="Mird"/>
  </file>
  <file name="index.xml">
    <!-- Import all remaining crawled data to this file: -->
    <target module=""/>
  </file>
</dir>
