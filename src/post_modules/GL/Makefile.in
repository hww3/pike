# $Id: Makefile.in,v 1.17 2006/06/21 16:02:43 grubba Exp $
@make_variables@
VPATH=@srcdir@
OBJS=top.o @AUTO_OBJS@
MODULE_LDFLAGS=@LDFLAGS@ @LIBS@
CONFIG_HEADERS=@CONFIG_HEADERS@
MODULE_CFLAGS=@X_CFLAGS@

override:
	@echo "Original CFLAGS:$(CFLAGS)"
	@echo "Modified CFLAGS: `echo ' $(CFLAGS) ' | sed -e 's/ -g[^ ]*//g' -e 's/ -W[^ ]*//g' -e 's/ -O[^ ]*//g'` -O `echo ' $(CFLAGS) ' | sed -e 's/.*\( -gcc-version[^ ]*\).*/\1/'`"
	@$(MAKE) \
	  CFLAGS="`echo ' $(CFLAGS) ' | sed -e 's/ -g[^ ]*//g' -e 's/ -W[^ ]*//g' -e 's/ -O[^ ]*//g'` -O `echo ' $(CFLAGS) ' | sed -e 's/.*\( -gcc-version[^ ]*\).*/\1/'`" \
	  all

@dynamic_module_makefile@

auto.c : $(SRCDIR)/auto.c.in $(SRCDIR)/features.pike $(SRCDIR)/constants.pike $(SRCDIR)/gen.pike
	$(RUNPIKE) $(SRCDIR)/gen.pike < $(SRCDIR)/auto.c.in > auto.c

# Compatibility with stupid makes..
auto.o: auto.c

@dependencies@
