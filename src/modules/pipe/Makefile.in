SRCDIR=@srcdir@
VPATH=@srcdir@:@srcdir@/../..:../..
PREFLAGS=-I$(SRCDIR) -I$(SRCDIR)/../.. -I../..
CFLAGS=$(PREFLAGS) $(OTHERFLAGS) @DEFS@

FILES=pipe.o

pipe.a: $(FILES)
	-rm -f pipe.a
	ar cq pipe.a $(FILES)
	-@RANLIB@ pipe.a

clean:
	-rm -f *.o *.a

depend:
	gcc -MM $(PREFLAGS) $(SRCDIR)/*.c | $(FIXDEP) $(SRCDIR)
# Depencies begin here
pipe.o: pipe.c \
 config.h machine.h \
 global.h \
 config.h \
 port.h \
 stralloc.h \
 macros.h \
 object.h \
 svalue.h \
 constants.h \
 hashtable.h \
 las.h \
 dynamic_buffer.h \
 program.h \
 interpret.h \
 error.h
