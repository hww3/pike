SRCDIR=@srcdir@
VPATH=@srcdir@:@srcdir@/../..:../..
PREFLAGS=-I$(SRCDIR) -I$(SRCDIR)/../.. -I../..
CFLAGS=$(PREFLAGS) $(OTHERFLAGS)

FILES=sprintf.o

sprintf.a: $(FILES)
	-rm -f sprintf.a
	ar cq sprintf.a $(FILES)
	-@RANLIB@ sprintf.a

clean:
	-rm -f *.o *.a

depend:
	gcc -MM $(PREFLAGS) $(SRCDIR)/*.c | $(FIXDEP) $(SRCDIR)/Makefile.in


#Dependencies begin here, DO NOT REMOVE THIS LINE!!!!
sprintf.o: sprintf.c \
  global.h machine.h \
  config.h \
  debug.h \
  port.h \
  error.h \
  svalue.h \
  array.h \
  las.h \
  dynamic_buffer.h \
  program.h \
  stralloc.h \
  lpc_types.h \
  add_efun.h \
  hashtable.h \
  interpret.h
