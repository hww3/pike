@make_variables@
VPATH=@srcdir@:@srcdir@/../../..:../../..
OBJS = pnm.o x.o xwd.o png.o any.o bmp.o tga.o pcx.o xbm.o _xpm.o ilbm.o iff.o xcf.o hrz.o avs.o psd.o wbf.o ras.o pvr.o tim.o dsi.o

RANLIB=@RANLIB@

MODULE_CPPFLAGS=-I$(SRCDIR)/.. -I..

@dynamic_module_makefile@

all: encodings.a

encodings.a : $(OBJS)
	rm -f encodings.a
	$(AR) cq encodings.a $(OBJS)
	$(RANLIB) encodings.a

pike: all
	cd ../../..; $(MAKE)

pure: all
	cd ../../..; $(MAKE) pure

@dependencies@
