test_do([[
  foreach( ",I,II,III,IV,V,VI,VII,VIII,IX,X,XI,XII,XIII,XIV,XV,XVI,XVII,XVIII,XIX,XX"/",";
           int t; string res)
    if(String.int2roman(t)!=res) error("%s should be %s\n", String.int2roman(t), res);
]])
test_eq(String.int2roman(100),"C")
